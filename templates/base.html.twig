<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <meta name="viewport" content="width=device-width, initial-scale=1">
	<meta http-equiv="x-ua-compatible" content="ie=edge">
        {% block stylesheets %}
        <!-- Theme Font -->
	<link rel="preload" type="text/css" href="{{ asset("assets/html/css/theme-font.min.css")}}">

	<!-- Bootstrap CSS -->
	<link rel="stylesheet" type="text/css" href="{{ asset("assets/html/Bootstrap/dist/css/bootstrap.css")}}">

	<!-- Main Styles CSS -->
	<link rel="stylesheet" type="text/css" href="{{ asset("assets/html/css/main.min.css") }}">

	<!-- Main RTL CSS -->
	<!--<link rel="stylesheet" type="text/css" href="{{ asset("assets/html/css/rtl.min.css") }}">-->
        {% endblock %}

        {% block javascripts %}
            <!-- JS Scripts -->
<script src="{{ asset("asset/js/jQuery/jquery-3.5.1.min.js")}}"></script>

<script src="{{ asset("assets/html/js/libs/jquery.mousewheel.min.js")}}"></script>
<script src="{{ asset("assets/html/js/libs/perfect-scrollbar.min.js")}}"></script>
<script src="{{ asset("assets/html/js/libs/imagesloaded.pkgd.min.js")}}"></script>
<script src="{{ asset("assets/html/js/libs/material.min.js")}}"></script>
<script src="{{ asset("assets/html/js/libs/selectize.min.js")}}"></script>
<script src="{{ asset("assets/html/js/libs/swiper.jquery.min.js")}}"></script>
<script src="{{ asset("assets/html/js/libs/moment.min.js")}}"></script>
<script src="{{ asset("assets/html/js/libs/daterangepicker.min.js")}}"></script>
<script src="{{ asset("assets/html/js/libs/isotope.pkgd.min.js")}}"></script>
<script src="{{ asset("assets/html/js/libs/ajax-pagination.min.js")}}"></script>
<script src="{{ asset("assets/html/js/libs/jquery.magnific-popup.min.js")}}"></script>
<script src="{{ asset("assets/html/js/libs/aos.min.js")}}"></script>

<script src="{{ asset("assets/html/js/main.js")}}"></script>
<script src="{{ asset("assets/html/js/libs-init/libs-init.js")}}"></script>

<script src="{{ asset("assets/html/Bootstrap/dist/js/bootstrap.bundle.min.js")}}"></script>

<!-- SVG icons loader -->
<script src="{{ asset("asset/html/js/svg-loader.js")}}"></script>
<!-- /SVG icons loader -->
        {% endblock %}
    </head>
    <body class="body-bg-white>
        {{ include('includes/header.html.twig') }}
        <section class="tout_tout">
                {% block body %}
                {% endblock %}
        </section>
        {{ include('includes/footer.html.twig') }}
    </body>
</html>
